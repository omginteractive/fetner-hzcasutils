{
  "name": "hz-core",
  "version": "0.0.3",
  "description": "HZ's internal library of React Components",
  "main": "index.js",
  "repository": "git@github.com:hzdg/hz-core.git",
  "author": "elainen <elaineh@hzdg.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "node ./scripts/netlify-preinstall.js",
    "build": "./node_modules/.bin/lerna bootstrap --loglevel=error && yarn build-packages",
    "build-docz": "yarn build && ./node_modules/.bin/docz build",
    "build-packages": "./node_modules/.bin/build-packages",
    "clean": "yarn build-packages --clean",
    "commit": "./node_modules/.bin/git-cz",
    "create-package": "./node_modules/.bin/create-package",
    "develop": "yarn build-packages && ./node_modules/.bin/docz dev",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./packages",
    "preversion": "yarn test && yarn build -f",
    "start": "yarn develop",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "test": "yarn build && ./node_modules/.bin/jest",
    "test-debug": "yarn build && node --inspect-brk ./node_modules/.bin/jest",
    "test-publish": "yarn build && ./node_modules/.bin/test-publish",
    "test-publish-debug": "yarn build && node --inspect-brk ./node_modules/.bin/test-publish"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/",
    "@hzdg:registry": "https://npm.pkg.github.com/"
  },
  "workspaces": [
    "packages/animations/*",
    "packages/behaviors/*",
    "packages/conventional-commits/*",
    "packages/simple-actions/*",
    "packages/utilities/*",
    "packages/ui-components/*"
  ],
  "config": {
    "commitizen": {
      "path": "@hzdg/commitizen-config"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.4.3",
    "@commitlint/cli": "^8.3.5",
    "@testing-library/dom": "^7.10.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.0",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^11.2.0",
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/invariant": "^2.2.31",
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.2.3",
    "@types/npm-package-arg": "6.1.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/react-motion": "^0.0.28",
    "@types/shallowequal": "^1.1.1",
    "@types/warning": "^3.0.0",
    "@types/zen-observable": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-dev-expression": "^0.2.2",
    "chalk": "^2.4.2",
    "commitizen": "^4.0.4",
    "docz": "^1.1.0",
    "docz-core": "^1.1.0",
    "docz-plugin-netlify": "^1.1.4",
    "docz-theme-default": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-hzdg": "^2.1.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-typescript": "^0.14.0",
    "glob": "^7.1.2",
    "husky": "^3.0.5",
    "invariant": "^2.2.4",
    "jest": "^26.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-snapshot-serializer-raw": "^1.1.0",
    "jest-watch-typeahead": "^0.4.2",
    "jest-watch-yarn-workspaces": "^1.1.0",
    "lerna": "^3.13.1",
    "ncp": "^2.0.0",
    "netlify-cli": "^2.46.0",
    "prettier": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-motion": "^0.5.2",
    "react-spring": "^8.0.15",
    "react-test-renderer": "^16.9.0",
    "semver": "^6.0.0",
    "symbol-iterator": "^1.0.2",
    "typescript": "^3.8.2",
    "yargs": "^13.2.2"
  },
  "resolutions": {
    "docz-plugin-netlify/**/docz-core": "^1.1.0",
    "@babel/core": "^7.8.6",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3"
  }
}
